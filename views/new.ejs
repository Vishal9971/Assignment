<%- include('./partials/header') %>
<h1>Fill Details</h1>
<form action="/new" method="POST">
  <div class="d-flex flex-wrap m-2">
    <label for="name">NAME : </label>
    <input class="mx-3" type="text" id="name" name="name" />
  </div>
  <div class="d-flex flex-wrap m-2">
    <label for="img">IMG-URL : </label>
    <textarea class="mx-3" rows="3" cols="50" type="text" id="img" name="img"></textarea>
  </div>
  <div class="d-flex flex-wrap m-2">
    <label for="price">Price : </label>
    <input class="mx-3" type="text" id="price" name="price" />
  </div>
  <button class="d-flex flex-wrap m-2 btn btn-primary" type="submit">ADD</button>
</form>
<%- include('./partials/footer') %>
